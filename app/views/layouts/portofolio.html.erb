<!DOCTYPE html>
<html>
  <head>
    <title><%= @default_title %></title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>


    <%= stylesheet_link_tag    'portofolios', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>

    <%= render 'shared/nav' %>
    <div>
    <%= login_helper %>
    <%= yield %>

    <%= session.inspect %>
  </body>
</html>
